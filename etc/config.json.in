{
    "backend": {
        "module": "wf-moray-backend",
        "opts": {
            "url": "WF_MORAY_URL",
            "extra_fields": {
                "wf_workflows": {},
                "wf_jobs": {
                    "vm_uuid": {
                        "type": "string",
                        "index": true,
                        "unique": false
                    },
                    "server_uuid": {
                        "type": "string",
                        "index": true,
                        "unique": false
                    }
                },
                "wf_jobs_info": {}
            }
        }
    },
    "api": {
        "port": 80,
        "wf_extra_params": [],
        "job_extra_params": ["vm_uuid", "server_uuid"]
    },
    "maxHttpSockets": 100,
    "logLevel": "info",
    "maxInitRetries": 50,
    "runner": {
        "identifier": "WF_RUNNER_UUID",
        "forks": WF_NUM_FORKS,
        "run_interval": WF_RUN_INTERVAL,
        "sandbox": {
            "modules": {
                "urlModule": "url",
                "http": "http",
                "uuid": "node-uuid",
                "restify": "restify",
                "async": "async",
                "sdcClients": "sdc-clients",
                "verror": "verror"
            },
            "ufdsUrl": "UFDS_URL",
            "ufdsDn": "UFDS_LDAP_ROOT_DN",
            "ufdsPassword": "UFDS_LDAP_ROOT_PW",
            "dapiUrl": "DAPI_URL",
            "napiUrl": "NAPI_URL",
            "cnapiUrl": "CNAPI_URL",
            "vmapiUrl": "VMAPI_URL",
            "imgapiUrl": "IMGAPI_URL",
            "fwapiUrl": "FWAPI_URL"
        }
    }
}

