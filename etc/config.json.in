{
    "backend": {
        "module": "wf-moray-backend",
        "opts": {
            "url": "WF_MORAY_URL"
        }
    },
    "api": {
        "port": 80
    },
    "maxHttpSockets": 100,
    "logLevel": "debug",
    "maxInitRetries": 50,
    "runner": {
        "identifier": "WF_RUNNER_UUID",
        "forks": WF_NUM_FORKS,
        "run_interval": WF_RUN_INTERVAL,
        "sandbox": {
            "modules": {
                "urlModule": "url",
                "http": "http",
                "uuid": "node-uuid",
                "restify": "restify",
                "async": "async",
                "sdcClients": "sdc-clients",
                "verror": "verror"
            },
            "ufdsUrl": "UFDS_URL",
            "ufdsDn": "UFDS_LDAP_ROOT_DN",
            "ufdsPassword": "UFDS_LDAP_ROOT_PW",
            "dapiUrl": "DAPI_URL",
            "napiUrl": "NAPI_URL",
            "cnapiUrl": "CNAPI_URL",
            "vmapiUrl": "VMAPI_URL",
            "imgapiUrl": "IMGAPI_URL",
            "fwapiUrl": "FWAPI_URL"
        }
    }
}

