{
    "backend": {
        "module": "wf-moray-backend",
        "opts": {
            "url": "WF_MORAY_URL"
        }
    },
    "api": {
        "port": 80,
        "cert": "/opt/smartdc/workflow/ssl/cert.pem",
        "key": "/opt/smartdc/workflow/ssl/key.pem",
        "user": "HTTP_ADMIN_USER",
        "password": "HTTP_ADMIN_PW"
    },
    "maxHttpSockets": 100,
    "logLevel": "info",
    "maxInitRetries": 10,
    "runner": {
        "identifier": "WF_RUNNER_UUID",
        "forks": WF_NUM_FORKS,
        "run_interval": WF_RUN_INTERVAL,
        "sandbox": {
            "modules": {
                "http": "http",
                "uuid": "node-uuid",
                "restify": "restify",
                "sdcClients": "sdc-clients"
            },
            "ufds_url": "UFDS_URL",
            "ufds_dn": "UFDS_LDAP_ROOT_DN",
            "ufds_password": "UFDS_LDAP_ROOT_PW",
            "dapi_url": "DAPI_URL",
            "napi_url": "NAPI_URL",
            "napi_username": "NAPI_HTTP_ADMIN_USER",
            "napi_password": "NAPI_HTTP_ADMIN_PW",
            "cnapi_url": "CNAPI_URL",
            "vmapi_url": "VMAPI_URL"
        }
    }
}

